{
  "name": "vscode-insert-calendar",
  "displayName": "vscode-insert-calendar",
  "description": "This extension inserts formatted calendar into the editor.",
  "version": "0.0.1",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license":"MIT",
  "homepage": "https://github.com/standard-software/vscode-insert-calendar",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-insert-calendar"
  },
  "bugs": {
    "url": "https://github.com/standard-software/vscode-insert-calendar/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": ["calendar"],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "DateTimeCalendar",
      "properties": {
        "InsertCalendar.CustomDayOfWeekLong": {
          "default": {
            "Sun": "日曜日",
            "Mon": "月曜日",
            "Tue": "火曜日",
            "Wed": "水曜日",
            "Thu": "木曜日",
            "Fri": "金曜日",
            "Sat": "土曜日"
          },
          "description": "Insert Calendar : CustomDayOfWeekLong"
        },
        "InsertCalendar.CustomDayOfWeekShort": {
          "default": {
            "Sun": "日",
            "Mon": "月",
            "Tue": "火",
            "Wed": "水",
            "Thu": "木",
            "Fri": "金",
            "Sat": "土"
          },
          "description": "Insert Calendar : CustomDayOfWeekShort"
        },

        "InsertCalendar.SquareCalendar": {
          "type": "array",
          "default": [
            {
              "title": "MMMMM YYYY ddd D \"Space2\"",
              "headerFormat": "LMMMMM              YYYY",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "SD",
              "space": "  ",
              "otherMonthDate": false,
              "startDayOfWeek": "Sun"
            },
            {
              "title": "MMMM YYYY ddd DD \"Space2 OtherMonth\"",
              "headerFormat": "MMMM YYYY",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "DD",
              "space": "  ",
              "otherMonthDate": true,
              "startDayOfWeek": "Sun"
            },
            {
              "title": "YYYY MMMMM dd D \"Space1\"",
              "headerFormat": "YYYY       RMMMMM",
              "dayOfWeekFormat": "dd",
              "dateFormat": "SD",
              "space": " ",
              "otherMonthDate": false,
              "startDayOfWeek": "Sun"
            },
            {
              "title": "YYYY/MM ddd DD \"Space1 OtherMonth\"",
              "headerFormat": "YYYY/MM",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "DD",
              "space": " ",
              "otherMonthDate": true,
              "startDayOfWeek": "Sun"
            },
            {
              "title": "\"Start monday : \"MMMMM YYYY ddd D \"Space2\"",
              "headerFormat": "LMMMMM              YYYY",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "SD",
              "space": "  ",
              "otherMonthDate": false,
              "startDayOfWeek": "Mon"
            },
            {
              "title": "\"Start monday : \"MMMM YYYY ddd DD \"Space2 OtherMonth\"",
              "headerFormat": "MMMM YYYY",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "DD",
              "space": "  ",
              "otherMonthDate": true,
              "startDayOfWeek": "Mon"
            },
            {
              "title": "\"Start monday : \"YYYY MMMMM dd D \"Space1\"",
              "headerFormat": "YYYY       RMMMMM",
              "dayOfWeekFormat": "dd",
              "dateFormat": "SD",
              "space": " ",
              "otherMonthDate": false,
              "startDayOfWeek": "Mon"
            },
            {
              "title": "\"Start monday : \"YYYY/MM ddd DD \"Space1 OtherMonth\"",
              "headerFormat": "YYYY/MM",
              "dayOfWeekFormat": "ddd",
              "dateFormat": "DD",
              "space": " ",
              "otherMonthDate": true,
              "startDayOfWeek": "Mon"
            }
          ],
          "description": "Insert Calendar : Square Calendar"
        },
        "InsertCalendar.VerticalCalendar": {
          "type": "array",
          "default": [
            {
              "title": "YYYY/MM|  DD  ddd",
              "headerFormat": "YYYY/MM",
              "lineFormat": "  DD ddd",
              "startDayOfWeek": "Sun"
            },
            {
              "title": "MMM YYYY|  YYYY/MM/DD ddd",
              "headerFormat": "MMM YYYY",
              "lineFormat": "  YYYY/MM/DD ddd",
              "startDayOfWeek": "Sun"
            },
            {
              "title": "\"Start monday : \"YYYY/MM|  DD  ddd",
              "headerFormat": "YYYY/MM",
              "lineFormat": "  DD ddd",
              "startDayOfWeek": "Mon"
            },
            {
              "title": "\"Start monday : \"MMM YYYY|  YYYY/MM/DD ddd",
              "headerFormat": "MMM YYYY",
              "lineFormat": "  YYYY/MM/DD ddd",
              "startDayOfWeek": "Mon"
            }
          ],
          "description": "Insert Calendar : Vertical Calendar"
        }
      }
    },
    "commands": [
      {
        "command": "vscode-insert-calendar.SquareCalendarThisMonth",
        "title": "Insert Calendar : Insert Square Calendar : This Month"
      },
      {
        "command": "vscode-insert-calendar.VerticalCalendarThisMonth",
        "title": "Insert Calendar : Insert Vertical Calendar : This Month"
      },
      {
        "command": "vscode-insert-calendar.SelectCalendarType",
        "title": "Insert Calendar : Select Calendar Type..."
      },
      {
        "command": "vscode-insert-calendar.SelectMonth",
        "title": "Insert Calendar : Select Month..."
      }
    ]
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "npx -y vsce package && code --install-extension vscode-insert-calendar-0.0.1.vsix",
    "": ""
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
